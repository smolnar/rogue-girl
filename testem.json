{
  "framework": "mocha",
  "src_files": [
    "node_modules/chai/chai.js",
    "node_modules/sinon/pkg/sinon.js",
    "spec/setup.js",
    "dist/*_spec.js"
  ],
  "launchers": {
    "node": {
      "command": "./node_modules/.bin/mocha -r spec/setup.js -R tap dist/*_spec.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "node"
  ]
}
